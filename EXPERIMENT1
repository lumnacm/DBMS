//CREATE DATABASE INVOICE:

create database invoice;
Query OK, 1 row affected (0.01 sec)

mysql> use invoice;
Database changed

//TABLE CUSTOMER

create table customer(id int primary key,name varchar(20),address varchar(30),city varchar(50),ph_no int);
Query OK, 0 rows affected (0.03 sec)

mysql> desc customer;
