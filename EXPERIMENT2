3.INSERT VALUES IN TO INVOICE DATABASE

insert into customer values(1001,'lumna','abcd','calicut',98765432);
Query OK, 1 row affected (0.00 sec)

mysql> insert into customer values(1002,'thehzi','pqrs','ekm',98652982);
Query OK, 1 row affected (0.01 sec)

mysql> insert into customer values(1003,'shahala','xyz','tvm',76895643);
Query OK, 1 row affected (0.01 sec)

mysql> select * from customer;
